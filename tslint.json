{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single",
            "jsx-single"
        ],
        "semicolon": [
            true,
            "never"
        ],
        "space-before-function-paren": [
            true,
            "always"
        ],
        "no-shadowed-variable": false,
        "interface-name": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false
    },
    "rulesDirectory": []
}